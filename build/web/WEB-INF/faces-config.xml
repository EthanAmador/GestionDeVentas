<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    <managed-bean>
        <managed-bean-name>productoBean</managed-bean-name>
        <managed-bean-class>com.gerenciaProyecto.Beans.ProductoBean</managed-bean-class>
    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <description>Bean Para la configuración de aplicación</description>
        <managed-bean-name>configuracionBean</managed-bean-name>
        <managed-bean-class>com.gerenciaProyecto.Beans.ConfiguracionBean</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ingresoBeanLogin</managed-bean-name>
        <managed-bean-class>com.gerenciaProyecto.Beans.IngresoBeanLogin</managed-bean-class>
    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
        
    <managed-bean>
        <managed-bean-name>usuarioBean</managed-bean-name>
        <managed-bean-class>com.gerenciaProyecto.Beans.UsuarioBean</managed-bean-class>
    
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
